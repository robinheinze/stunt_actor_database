<h2>Change the name of your tag</h2>

<%= render('layouts/error', :object => @skill) %>

<form id="edit-form" role="form" action="/skills/<%= @skill.slug %>" method="post">
  <input name="_method" type="hidden" value="patch">
  <%= render('form') %>
  <div class="col-md-12">
    <button class="btn btn-primary" type="submit" form="edit-form">Save Changes!</button>
    <a href="/skills/<%= @skill.slug %>" class="btn btn-danger">Cancel</a>
  </div>
</form>
